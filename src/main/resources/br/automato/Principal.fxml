<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="br.automato.controller.PrincipalController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="tableView" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="colCod" prefWidth="75.0" text="Código" />
                <TableColumn fx:id="colToken" prefWidth="150.0" text="Token" />
                  <TableColumn fx:id="colDesc" prefWidth="200.0" text="Descrição" />
              </columns>
            </TableView>
         </center>
         <top>
            <Button mnemonicParsing="false" onAction="#loadFile" text="Adiciona Arquivo" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
